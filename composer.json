{
    "name": "ezsystems/ezpublish-community",
    "description": "eZ Publish 5 community distribution",
    "version": "5.0-dev",
    "homepage": "http://share.ez.no",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "eZ Publish dev-team & eZ Community",
            "homepage": "https://github.com/ezsystems/ezpublish-community/contributors"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ezsystems/ezpublish-kernel.git"
    }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.2.0-RC2",
        "symfony/assetic-bundle": "2.1.*",
        "symfony/monolog-bundle": "2.2.*",
        "symfony/swiftmailer-bundle": "2.2.*",
        "twig/extensions": "1.0.*",
        "ezsystems/ezpublish": "dev-master",
        "sensio/generator-bundle": "2.2.*",
        "egulias/listeners-debug-command-bundle": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "": "src/"
        }
    },
    "minimum-stability": "dev",
    "config": {
        "process-timeout": 3000
    }
}
